{
  "name": "@tammergard/eslint-config-react-typescript",
  "version": "0.0.0-development",
  "description": "A sharable ESLint config for React-TypeScript with personal preferences",
  "scripts": {
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint .",
    "review": "yarn install --frozen-lockfile && yarn format-check && yarn lint"
  },
  "dependencies": {
    "@tammergard/eslint-config-base": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.4.0"
  },
  "devDependencies": {
    "@tammergard/prettier-config": "1.5.0",
    "@tammergard/semantic-release-config-npm": "1.1.0",
    "eslint": "8.23.1",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5",
    "typescript": "4.8.3"
  },
  "peerDependencies": {
    "eslint": ">=8.12.0",
    "prettier": ">=2.6.1"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
