{
	"name": "@tammergard/eslint-config-react-typescript",
	"version": "3.0.0",
	"description": "A sharable ESLint config for React-TypeScript with personal preferences",
	"scripts": {
		"format": "prettier --write .",
		"format-check": "prettier --check .",
		"release": "pnpm changeset publish",
		"review": "pnpm install --frozen-lockfile && pnpm format-check"
	},
	"dependencies": {
		"@tammergard/eslint-config-base": "4.0.0",
		"@typescript-eslint/eslint-plugin": "5.57.0",
		"@typescript-eslint/parser": "5.57.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-plugin-jsx-a11y": "6.7.1",
		"eslint-plugin-react": "7.32.2",
		"eslint-plugin-react-hooks": "4.6.0"
	},
	"devDependencies": {
		"@changesets/cli": "2.26.1",
		"@tammergard/prettier-config": "2.0.0",
		"prettier": "2.8.7",
		"typescript": "5.0.2"
	},
	"peerDependencies": {
		"eslint": ">=8.37.0",
		"prettier": ">=2.8.7"
	},
	"main": "index.js",
	"files": [
		"index.js"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/filiptammergard/eslint-config-react-typescript.git"
	},
	"keywords": [
		"eslint",
		"config",
		"eslint-config",
		"styleguide",
		"react",
		"typescript"
	],
	"author": "Filip Tammerg√•rd <filip+npm@tammergard.se> (https://tammergard.se/)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/filiptammergard/eslint-config-react-typescript/issues"
	},
	"homepage": "https://github.com/filiptammergard/eslint-config-react-typescript#readme"
}
