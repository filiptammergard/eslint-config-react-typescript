{
	"name": "@tammergard/eslint-config-react-typescript",
	"version": "0.0.0-development",
	"description": "A sharable ESLint config for React-TypeScript with personal preferences",
	"scripts": {
		"format": "prettier --write .",
		"format-check": "prettier --check .",
		"lint": "eslint .",
		"review": "yarn install --frozen-lockfile && yarn format-check && yarn lint"
	},
	"dependencies": {
		"@tammergard/eslint-config-base": "3.1.1",
		"@typescript-eslint/eslint-plugin": "5.48.2",
		"@typescript-eslint/parser": "5.48.2",
		"eslint-config-airbnb": "19.0.4",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-jsx-a11y": "6.7.1",
		"eslint-plugin-react": "7.32.1",
		"eslint-plugin-react-hooks": "4.6.0"
	},
	"devDependencies": {
		"@tammergard/prettier-config": "2.0.0",
		"@tammergard/semantic-release-config-npm": "1.1.0",
		"eslint": "8.32.0",
		"prettier": "2.8.3",
		"semantic-release": "20.0.3",
		"typescript": "4.9.4"
	},
	"peerDependencies": {
		"eslint": ">=8.12.0",
		"prettier": ">=2.6.1"
	},
	"main": "index.js",
	"files": [
		"index.js"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/filiptammergard/eslint-config-react-typescript.git"
	},
	"keywords": [
		"eslint",
		"config",
		"eslint-config",
		"styleguide",
		"react",
		"typescript"
	],
	"author": "Filip Tammerg√•rd <filip+npm@tammergard.se> (https://tammergard.se/)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/filiptammergard/eslint-config-react-typescript/issues"
	},
	"homepage": "https://github.com/filiptammergard/eslint-config-react-typescript#readme"
}
